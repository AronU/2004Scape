[label,levelup_agility]
// Agility level up unlock messages
switch_int(stat_base(agility)) {
    case 20 : ~objbox(agilityarena_ticket, "You can now pass medium level @dbl@Agility Arena@bla@ obstacles.", 250, 0, 0);
    case 35 : ~objbox(steel_battleaxe, "You can now enter the @dbl@Barbarian Agility Course@bla@.", 250, 0, 0);
    case 40 : ~objbox(agilityarena_ticket, "You can now pass high level @dbl@Agility Arena@bla@ obstacles.", 250, 0, 0);
    case 52 : ~objbox(skull, "You can now enter the @dbl@Wilderness Agility Course@bla@.", 250, 0, ^objbox_height);
}
