[debugproc,alleast](int $distance)
def_coord $coord = coord;
huntall($coord, 20, 0);
while (huntnext = true) {
    if_close;
    if (p_finduid(uid) = false) { @please_finish; return; }
    p_teleport(movecoord(coord, max($distance, 1), 0, 0));
}
npc_huntall($coord, 20, 0);
while (npc_huntnext = true) {
    npc_tele(movecoord(npc_coord, max($distance, 1), 0, 0));
}

